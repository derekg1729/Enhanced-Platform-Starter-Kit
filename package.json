{
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "drizzle-kit push && next build",
    "format:write": "prettier --write \"**/*.{css,js,json,jsx,ts,tsx}\"",
    "format": "prettier \"**/*.{css,js,json,jsx,ts,tsx}\"",
    "start": "next start",
    "lint": "next lint",
    "test": "vitest run --pool=forks",
    "test:watch": "vitest --pool=forks",
    "test:ui": "vitest --ui",
    "test:coverage": "vitest run --coverage",
    "test:build": "vitest run tests/build.test.ts",
    "test:ci": "vitest run",
    "test:ga": "vitest run --pool=forks tests/unit/analytics/google-analytics*.unit.test.* tests/unit/analytics/page-view-tracker.unit.test.tsx tests/integration/analytics/google-analytics*.integration.test.*",
    "test:ga:build": "RUN_BUILD_TESTS=true vitest run --pool=forks tests/integration/analytics/google-analytics-build.integration.test.ts",
    "test:ga:deployment": "RUN_DEPLOYMENT_TESTS=true vitest run --pool=forks tests/integration/analytics/google-analytics-deployment.integration.test.ts",
    "typecheck": "tsc --noEmit",
    "pre-commit": "npm run check-env && npm run lint && npm run typecheck && npm run test && npm run build",
    "setup-hooks": "npx husky init && chmod +x .husky/pre-commit",
    "check-env": "node scripts/check-env-consistency.js",
    "update-anthropic-models": "ts-node scripts/update-anthropic-models.ts",
    "prepare": "husky"
  },
  "lint-staged": {
    "*.{js,jsx,ts,tsx}": [
      "eslint --fix",
      "prettier --write"
    ],
    "*.{css,json,md}": [
      "prettier --write"
    ]
